alt - return : /Applications/Alacritty.app/Contents/MacOS/alacritty

alt + shift - q : yabai -m window --close

alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east
alt - f : yabai -m window --toggle zoom-fullscreen

shift + alt - h : yabai -m window --warp west
shift + alt - j : yabai -m window --warp south
shift + alt - k : yabai -m window --warp north
shift + alt - l : yabai -m window --warp east
 
alt - e : yabai -m window --toggle split

alt - 1 : yabai -m space --focus 1 || yabai -m space --create
alt - 2 : yabai -m space --focus 2 || yabai -m space --create
alt - 3 : yabai -m space --focus 3 || yabai -m space --create
alt - 4 : yabai -m space --focus 4 || yabai -m space --create
alt - 5 : yabai -m space --focus 5 || yabai -m space --create
alt - 6 : yabai -m space --focus 6 || yabai -m space --create
alt - 7 : yabai -m space --focus 7 || yabai -m space --create
alt - 8 : yabai -m space --focus 8 || yabai -m space --create
alt - 9 : yabai -m space --focus 9 || yabai -m space --create
alt - 0 : yabai -m space --focus 10 || yabai -m space --create

shift + alt - 1 : yabai -m window --space 1
shift + alt - 2 : yabai -m window --space 2
shift + alt - 3 : yabai -m window --space 3
shift + alt - 4 : yabai -m window --space 4
shift + alt - 5 : yabai -m window --space 5

shift + alt - space : yabai -m window --toggle float && yabai -m window --grid 4:4:1:1:2:2

alt + shift - r : \
    /usr/bin/env osascript <<< \
        "display notification \"Restarting Yabai\" with title \"Yabai\""; \
    launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"

